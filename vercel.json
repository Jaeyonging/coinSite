{
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "https://api.upbit.com/$1"
        },
        {
            "source": "/wss/(.*)",
            "destination": "wss://api.upbit.com/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/"
        }
    ]
}